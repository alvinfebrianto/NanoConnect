{
  "name": "nanoconnect-app",
  "caches": [
    {
      "source": "/images/*",
      "cacheTtl": 86400
    },
    {
      "source": "/assets/*",
      "cacheTtl": 31536000
    }
  ],
  "headers": [
    {
      "source": "/*",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}
